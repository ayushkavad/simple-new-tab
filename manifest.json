{
  "manifest_version": 2,
  "name": "Digital Clock New Tab",
  "version": "1.0",
  "description": "Displays a digital clock on the new tab page.",
  "permissions": ["storage", "activeTab"],
  "browser_action": {
    "default_icon": {
      "16": "icons/icon16.jpg"
    },
    "default_title": "Digital Clock New Tab",
    "default_popup": "popup.html"
  },
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "icons": {
    "16": "icons/icon16.jpg"
  }
}

